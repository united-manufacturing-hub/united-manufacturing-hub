            "thresholdsStyle": {"mode": "off"}
          }
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 96}
    },
    {
      "id": 21,
      "title": "Filesystem Cache Hit Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "(sum(rate(umh_core_filesystem_ops_total{cache_status=\"cached\"}[5m])) / sum(rate(umh_core_filesystem_ops_total[5m]))) * 100",
          "legendFormat": "Cache Hit Rate %",
